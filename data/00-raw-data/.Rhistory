set.seed(909391)
# Define myattemps() function
mytoss = function(p){
u = runif(1)
x = as.numeric(u < p)
return(x)
}
myattempts = function(p){
failure = 0
while (mytoss(p) == 0){
failure = failure + 1
}
return(failure)
}
# Choose five p's
p = c(0.25,0.5,0.33,0.7,0.42)
# Simulations
n = 10000
rgeom = rgeom(n,p[1])
myattempt = replicate(n,myattempts(p[1]))
dgeom = dgeom(0:50,p[1])
attempt_table = sapply(table(myattempt), function(x){x/10000})
rgeom_table = sapply(table(rgeom), function(x){x/10000})
#simulation dataframe
simulation = data.frame(rgeom = rgeom_table[0:50], myattemp = attempt_table[0:50],dgeom = round(dgeom[1:50],4))
head(simulation)
